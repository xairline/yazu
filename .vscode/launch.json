{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}"
    },
    {
      "name": "Wails: Production yazu",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/yazu.exe",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Wails: Debug yazu",
      "type": "go",
      "request": "debug",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/yazu.exe",
      "preLaunchTask": "build debug",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Wails: Dev yazu",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/yazu.exe",
      "preLaunchTask": "build dev",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "showLog": true,
      "program": "${workspaceFolder}",
      "preLaunchTask": "build debug",
      "env": {},
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}